# Archivo de ejemplo de variables de entorno
# Copia este archivo a .env y actualiza con tus valores reales
# NUNCA commits el archivo .env al repositorio

# =============================================================================
# GOOGLE MAPS API KEYS
# =============================================================================
# Obtén tus API Keys en: https://console.cloud.google.com/

# API Key para desarrollo
GOOGLE_MAPS_API_KEY_DEV=AIzaSy...

# API Key para staging
GOOGLE_MAPS_API_KEY_STAGING=AIzaSy...

# API Key para producción
GOOGLE_MAPS_API_KEY_PRODUCTION=AIzaSy...

# API Key para Android
GOOGLE_MAPS_ANDROID_API_KEY=AIzaSy...

# API Key para iOS
GOOGLE_MAPS_IOS_API_KEY=AIzaSy...

# =============================================================================
# BACKEND API URLS
# =============================================================================

# URL base para desarrollo (local)
API_BASE_URL_DEV=http://localhost:3000/api/v1

# URL base para staging
API_BASE_URL_STAGING=https://staging-api.gpscommunity.com/api/v1

# URL base para producción
API_BASE_URL_PRODUCTION=https://api.gpscommunity.com/api/v1

# WebSocket URL para desarrollo
WS_URL_DEV=ws://localhost:3000

# WebSocket URL para staging
WS_URL_STAGING=wss://staging-api.gpscommunity.com

# WebSocket URL para producción
WS_URL_PRODUCTION=wss://api.gpscommunity.com

# =============================================================================
# FIREBASE (Generado automáticamente por flutterfire configure)
# =============================================================================
# Estas variables se configuran automáticamente en firebase_options.dart
# No es necesario configurarlas aquí manualmente

# =============================================================================
# OTRAS CONFIGURACIONES
# =============================================================================

# Habilitar logs de debug
DEBUG_MODE=true

# Intervalo de actualización de ubicación (en segundos)
LOCATION_UPDATE_INTERVAL=10

# Precisión de ubicación (en metros)
LOCATION_ACCURACY=10
